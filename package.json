{
  "name": "m3rch-clothing-server",
  "version": "1.0.0",
  "engines": {
    "node": "11.13.0",
    "npm": "6.7.0"
  },
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "apollo-boost": "^0.4.7",
    "body-parser": "^1.18.3",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "^4.16.4",
    "graphql": "^14.6.0",
    "lodash": "^4.17.15",
    "react-apollo": "^3.1.3",
    "stripe": "8.14.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  }
}
