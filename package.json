{
  "name": "m3rch-clothing-server",
  "version": "1.0.0",
  "engines": {
    "node": "16.17.0",
    "npm": "8.15.0"
  },
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "preinstall": "apt-get install python3"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "^4.16.4",
    "sass": "^1.80.4",
    "stripe": "8.14.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  }
}
